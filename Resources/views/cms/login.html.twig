                    <li>
                        <label for="">Password:</label>
                        <input onkeypress="subForm(event, document.getElementById('authform'));" type="password" name="password" class="loginTextfield" value=""/>
                    </li>
                    <li>
                        <input type="checkbox" id="remember" class="checkbox" name="rememberme" value="1"/>
                        <label for="remember">Remember me?</label>
                    </li>
                    <li>
                        <input type="submit" class="button submit" name="" value="Sign in &rsaquo;"/>
                    </li>


            </form>

