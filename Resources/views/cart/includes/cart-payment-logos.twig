{% set paymentLogos = model.active('PaymentLogo') %}

{% if paymentLogos|length %}
    <ul class="list-reset items-center flex">
        {% for itm in paymentLogos %}
            <li class="mr-1"><img src="/images/assets/{{ itm.image }}/1" alt="{{ itm.title }}"></li>
        {% endfor %}
    </ul>
{% endif %}